<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:int="http://www.springframework.org/schema/integration"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd
		http://www.springframework.org/schema/integration http://www.springframework.org/schema/integration/spring-integration-4.1.xsd">


<bean id="messageLogger" class="org.springframework.boot.firehose.integration.test.MessageLogger"></bean>
<bean id="httpMetricService" class="org.springframework.boot.firehose.integration.HttpMetricService"></bean>
<bean id="counterMetricService" class="org.springframework.boot.firehose.integration.CounterMetricService"/>
<bean id="genericMetricService" class="org.springframework.boot.firehose.integration.GenericMetricService"/>
<bean id="metricRegistry" class="com.codahale.metrics.MetricRegistry"/>

</beans>
